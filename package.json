{
  "name": "online-shop-api",
  "version": "1.0.0",
  "description": "Minimalistic backend API for an online shop",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "drizzle-kit up:pg",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": ["api", "shop", "drizzle", "nodejs"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "drizzle-orm": "^0.29.3",
    "postgres": "^3.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "drizzle-kit": "^0.20.9"
  }
} 